!use scott
!set outputformat mysql

select *
from dept
where cast(deptno as bigint) in (
  select count(*)
  from emp
  where comm is null);
+--------+------------+----------+
| DEPTNO | DNAME      | LOC      |
+--------+------------+----------+
|     10 | ACCOUNTING | NEW YORK |
+--------+------------+----------+
(1 row)

!ok

